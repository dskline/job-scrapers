{
  "name": "@dskline/job-scrapers",
  "version": "1.0.0",
  "description": "A daily job availability reporting tool",
  "author": "Spencer Kline (dskline1@gmail.com)",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/** --fix --ext .js,.ts,.tsx",
    "prettier-format": "prettier --write src"
  },
  "repository": "https://github.com/dskline/job-scrapers",
  "dependencies": {
    "@icons/material": "0.4.1",
    "@next/bundle-analyzer": "9.4.4",
    "@reduxjs/toolkit": "1.4.0",
    "classnames": "2.2.6",
    "convict": "6.0.0",
    "graphql": "15.3.0",
    "graphql-request": "2.0.0",
    "heroicons": "0.3.7",
    "isomorphic-ws": "4.0.1",
    "next": "11.1.0",
    "next-manifest": "3.0.1",
    "next-pwa": "3.1.1",
    "next-redux-wrapper": "6.0.2",
    "next-urql": "1.1.0",
    "react": "16.13.1",
    "react-cloudinary-lazy-image": "1.3.4",
    "react-dom": "16.13.1",
    "react-ga": "3.1.2",
    "react-hotkeys-hook": "2.2.2",
    "react-icons": "3.10.0",
    "react-is": "16.13.1",
    "react-redux": "7.2.1",
    "react-star-ratings": "2.3.0",
    "sass": "1.26.10",
    "tailwindcss": "1.6.0",
    "urql": "1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.6",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@svgr/webpack": "5.4.0",
    "@types/convict": "5.2.1",
    "@types/node": "13.13.5",
    "@types/react": "16.9.43",
    "@types/react-redux": "7.1.9",
    "@types/workbox-window": "4.3.3",
    "autoprefixer": "9.8.5",
    "eslint": "6.8.0",
    "eslint-config-dskline": "1.2.8",
    "husky": "4.2.5",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "eslintConfig": {
    "extends": [
      "dskline"
    ],
    "ignorePatterns": [
      "**/generated/**/*"
    ],
    "settings": {
      "polyfills": [
        "fetch"
      ]
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "browserslist": [
    "last 2 versions, > 0.2%, not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  }
}
